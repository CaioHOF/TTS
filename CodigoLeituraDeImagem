      "source": [
        "# Função para somar os valores RGB de todos os pixels da imagem\n",
        "def s_pixels(imagem):\n",
        "  largura, altura = imagem.size\n",
        "  s_r, s_g, s_b = 0, 0, 0\n",
        "  try:\n",
        "    for y in range(altura):\n",
        "      for x in range(largura):\n",
        "        r, g, b = imagem.getpixel((x, y))\n",
        "        s_r += r\n",
        "        s_g += g\n",
        "        s_b += b\n",
        "  except:\n",
        "        s_r += 0\n",
        "        s_g += 0\n",
        "        s_b += 0\n",
        "\n",
        "  return s_r, s_g, s_b\n",
        "\n",
        "for x in range(len(df)):\n",
        "#for x in range(2):\n",
        "  # URL da imagem\n",
        "  image_url = df['image_url'][x]\n",
        "\n",
        "  # Realize o download da imagem\n",
        "  response = requests.get(image_url)\n",
        "\n",
        "  # Verifique se o download foi bem-sucedido\n",
        "  if response.status_code == 200:\n",
        "      # Converta o conteúdo da resposta em um objeto de imagem\n",
        "      image = Image.open(BytesIO(response.content))\n",
        "\n",
        "      # Obter a soma dos valores RGB de todos os pixels\n",
        "      s_r, s_g, s_b = s_pixels(image)\n",
        "      s_f = s_r + s_g + s_b\n",
        "      l2 = [df['nome'][x],s_f]\n",
        "      l.append(l2)\n",
        "  else:\n",
        "      print(\"Falha ao baixar a imagem\")\n",
        "      l.append(None)"
      ]
    },
